{
    "MainWindow_defaultWindowSize": "1100x600",
    "TextArea_backgroundColor": "#000000",
    "TextArea_selectBackgroundColor": "#303030",
    "TextArea_textColor": "#FFFFFF",
    "TextArea_fontFamily": "Consolas",
    "TextArea_fontSize": 10,
    "TextArea_maxLineBuffer": 4000,
    "LogFile_logFilePath": "Logs",
    "LogFile_logFileBaseName": "SerialLog_",
    "LogFile_logFileTimestamp": "%Y.%m.%d_%H.%M.%S",
    "LineColorMap": {
        "LineColorMap00": {
            "regex": "Main::.*",
            "color": "#1ae675"
        },
        "LineColorMap01": {
            "regex": "GUI::.*",
            "color": "#79ABFF"
        },
        "LineColorMap02": {
            "regex": "TM::.*=>",
            "color": "#cf7ce0"
        },
        "LineColorMap03": {
            "regex": "GFH=>",
            "color": "#ff4848"
        }
    }
}